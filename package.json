{
  "name": "mttk-vue-wrap",
  "version": "0.3.0",
  "description": "An alternative way of wrapping vue 3 component with pure JS",
  "main": "./dist/mttk-vue-wrap.umd.js",
  "module": "./dist/mttk-vue-wrap.mjs",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mttk-vue-wrap.js",
      "require": "./dist/mttk-vue-wrap.umd.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist",
    "packages/index.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "vite build"
  },
  "dependencies": {
    "@types/vue": "^2.0.0",
    "@vueuse/core": "^10.7.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3",
    "unplugin-vue-define-options": "^1.2.1",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.0",
    "vue-tsc": "^2.2.0"
  }
}
